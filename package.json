  "dependencies": {
    "anchor-js": "5.0.0",
    "bootstrap": "5.3.3",
    "jquery": "3.7.1",
    "@popperjs/core": "2.11.8"
  },
  "devDependencies": {
    "broken-link-checker": "0.7.8",
    "http-server": "14.1.1",
    "npm-run-all": "6.1.2"  // Fixed typo: changed "npm-run-all2" to "npm-run-all"
  },
  "scripts": {
    "serve": "http-server --silent -p 8888",  // Serve the site
    "check-links": "blc --exclude-external --filter-level 2 --follow --get --ordered --recursive --host-requests 4 --input http://localhost:8888/ --exit-code",  // Check for broken links with exit code
    "test-links": "npm-run-all --parallel --race \"npm run serve\" \"npm run check-links\""  // Run server and check links simultaneously
  },
  "homepage": "https://jenkins.io"
}


