%ol
  - site.handbook.chapters.each do |chapter|
    %li{:style => 'font-size: 24px;'}
      %a{:href => chapter.key}
        = chapter.title
      %ol
        - chapter.sections.each do |section|
          %li{:style => 'font-size: 16px;'}
            %a{:href => File.join(chapter.key, section.key)}
              = section.title

